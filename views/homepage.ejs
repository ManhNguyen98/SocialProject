                <img src="/images/avt1.jpg">
              </div>
              <div class="userStatusText">
                <form action="/status" method="post">
                <input type="text" placeholder="Hôm nay của bạn như thế nào ?" class="statusText">
              <div  class="btnStatus">
                <button type="submit" class="btn btn-default">Chia sẻ <span class="glyphicon glyphicon-share-alt"></span></button>
              </div>
              </form>
              </div>
            </div>
            <div class="notice">
              
            </div>
